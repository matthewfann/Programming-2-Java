package exercises;

/**
 * 
 * @author Jamal
 *
 */
public class Kennel {

	public static void main(String[] args) {

		Dog[] dogs = new Dog[4];

		dogs[0] = new Dog("Charlie", 2);
		dogs[1] = new Dog("Tody", 5);
		dogs[2] = new Dog("Oscar", 3);
		dogs[3] = new Dog("Max", 6);

		dogs[3].setAge(4);

		for (int i = 0; i < dogs.length; i++) {
			System.out.println(dogs[i]);
		}
	
	}

}
